<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="style.css" rel="stylesheet" type="text/css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">

</head>

<body>
   <div class="row">
    <div class="col-2">
      <nav id="sidebarMenu" class="sidebar">
        <div class="sidebar-sticky pt-3">
            <p class="d-flex justify-content-center align-items-center">
                <a href="#">

                </a>
            </p>

            <img class="img-fluid rounded-circle mx-auto" style="margin-top:50px;" src="logo.png"  /></span>

            <ul class="nav nav-pills flex-column mx-4">
                <li class="nav-item mt-5 mb-2">
                    <a class="nav-link active" href="#">

                    Add Teacher <span class="sr-only"></span>
                    </a>
                </li>
                <li class="nav-item mb-2">
                    <a class="nav-link" href="#">

Add Student
                    </a>
                </li>
                <li class="nav-item mb-2">
                    <a class="nav-link" href="#">

                    View Teacher
                    </a>
                </li>
                <li class="nav-item mb-2">
                    <a class="nav-link" href="#">
                    View Student

                    </a>
                </li>
                <li class="nav-item mb-2">
                    <a class="nav-link" href="#">
                    Logout

                    </a>
                </li>
            </ul>
        </div>
      </nav>
    </div>
     <div class="col-10">

        <h1 class="text-center" style="color:#f768a1;">Add Student</h1>
        <div class="col-md-6 offset-md-3">
            <form action="/student" method="POST">
                <div class="form-group row mt-4 mb-3">
                    <label class="form-label col-sm-2" for="title">Name</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="title" name="title" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>

                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="address">Address</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="address" name="address" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="mobile">Mobile</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="mobile" name="mobile" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="dob">Dob</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="dob" name="dob" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
               <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="gender">Gender</label>
                    <div class="col-sm-7">
                       <select class="form-control" name="gender">
                                <option disabled="true" selected>--  Select Option  --</option>
                                <option value="Male">Male</option>
                                <option value="Female">Female</option>
                            </select>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="email">Email</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="email" name="email" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="class">Class</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="class" name="class" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="class_teacher">Class Teacher</label>
                    <div class="col-sm-7">
                    <select class="form-control" name="class_teacher">
                        <% for(var i=0; i < user.length; i++) { %>
                          <option><%= user[i].title %></option>
                          <% } %>
                            </select>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="username">username</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="username" name="username" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="form-group row mb-3">
                    <label class="form-label col-sm-2" for="password">Password</label>
                    <div class="col-sm-7">
                    <input class="form-control" type="text" id="password" name="password" required>
                    </div>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                </div>

<center>
                    <div class="mb-3">
                        <button class="btn btn-success">Add Campground</button>
                    </div>
                </center>
            </form>
</body>

</html>
